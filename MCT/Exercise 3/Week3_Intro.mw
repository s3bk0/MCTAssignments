</Group>
<Group hide-input="false" hide-output="false" labelreference="L151" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">i := 'i';   # makes i a variable again</Text-field>
