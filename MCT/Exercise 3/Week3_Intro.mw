</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L141" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">quo((x^4-4),(x^2-2),x);  # divides polynomials </Text-field>
</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L142" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">iquo(23,2) ;  # divides the integers</Text-field>
</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L143" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">rem((x^4-4*x+3),(x^2-2),x);  # gives the remainder</Text-field>
</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L144" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">irem(23,2) ; # gives the integer remainder </Text-field>
