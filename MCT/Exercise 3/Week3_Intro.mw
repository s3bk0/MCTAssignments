</Group>
<Group hide-input="false" hide-output="false" labelreference="L146" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">eq1 := x^2 + 3*x -1 = a;  # assigns the equation</Text-field>
